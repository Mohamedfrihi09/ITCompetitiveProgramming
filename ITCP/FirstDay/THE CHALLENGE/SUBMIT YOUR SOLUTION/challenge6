program Pr;
 var
   t:array [1..100] of integer;
   i,n,c,f:integer;
begin
   writeln('donner la taille de vecteur');
   read(n);
  for i:=1 to n do
  begin
  writeln('t[',i,']=');
  readln(t[i]);
   end;
  writeln('donner le nombre que vous voulez etudier');
  readln(c);
   for i:=1 to n do
   if t[i]=c then
   f:=i;
   writeln('le nombre est dans la case num',f);
    if t[i]<>c then
   writeln('le nombre n''exicte pas');
     readln();
end.
           